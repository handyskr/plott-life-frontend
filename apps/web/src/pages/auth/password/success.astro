---
import Layout from '@layouts/Layout.astro';
import Header from '@components/template/Header.astro';
import Success from '@components/template/Success.astro';
---
<Layout subtitle="비밀번호 변경 완료">
  <Header
    slot='header'
    showBackIcon={true}
  />
  <Success
    title='비밀번호가 변경되었습니다'
    description='새로운 비밀번호로 로그인해주세요.'
    buttonText='로그인 화면으로'
    buttonLink='/auth/login'
  />
</Layout>
